  <!--==================== Install artificial parts =======================-->
  <RecipeDef ParentName="ExtendedSurgery">
    <defName>InstallArtificialNose</defName>
    <label>install artificial nose</label>
    <description>Installs artificial nose.</description>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <successfullyRemovedHediffMessage>{0} has successfully replaced {1}'s nose.</successfullyRemovedHediffMessage>
    <jobString>Installing artificial nose.</jobString>
    <workAmount>2000</workAmount>
    <surgerySuccessChanceFactor>1.30</surgerySuccessChanceFactor>
    <skillRequirements>
      <Artistic>5</Artistic>
      <Medicine>5</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>Plasteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Nose</li>
    </appliedOnFixedBodyParts>
    <addsHediff>ArtificialNose</addsHediff>
  </RecipeDef>
